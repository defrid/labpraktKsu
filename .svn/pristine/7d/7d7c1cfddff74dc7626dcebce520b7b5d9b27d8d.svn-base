$.extend(true, window, {
    "Slick": {
        "Validators": {
            "Integer": IntegerValidator
        }
    }
});


function IntegerValidator(options) {
    var validationFunction = new Function('value', 'row', options.function);

    this.validate = function(value, row){
        if (isNaN(value)) {
            return {
                valid: false,
                msg: "Please enter a valid integer"
            };
        }
        return {
            valid: true,
            msg: ''
        }
    }
};