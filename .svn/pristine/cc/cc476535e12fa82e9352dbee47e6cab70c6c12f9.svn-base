$.extend(true, window, {
    "Slick": {
        "Validators": {
            "Pattern": PatternValidator
        }
    }
});

function PatternValidator(options) {
    var regexPattern = new RegExp(options.pattern),
        message = options.msg;
    var validationFunction = new Function('value', 'row', options.function);

    this.validate = function(value, row){
        var isValid = regexPattern.test(value);
        return {
            valid: isValid,
            msg: (isValid ? '': message)
        }
        throw "Not implemented";
    }
};